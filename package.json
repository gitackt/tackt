{
  "name": "tackt",
  "homepage": "https://tackt-m.com/",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^26.0.17",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.5",
    "bulma": "^0.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.10",
    "node-sass": "^5.0.0",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid-system": "^7.0.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.1",
    "react-snap": "^1.23.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "routine": "sh routine.sh",
    "deploy": "sh deploy.sh",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint src --ext ts --ext tsx --ext js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
